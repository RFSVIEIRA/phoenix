<!-- Layout ScreenSize -->
<layout>


<!-- Shaders -->
	<image src="images/shaders/crt_border.png" width="stretch" height="stretch" alpha="1" layer="19" group="General" subgroup="SHADERS" active="false" shared="SCREEN, SCANLINES">
		<onEnter>
			<set duration=".001">
				<animate type="alpha" to="1"/>
			</set>
